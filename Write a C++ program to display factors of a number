#include<iostream>
using namespace std;
void factors(int num)
{
    if(num==0)
    {
        cout<<"factor of"<<num<<"is"<<0<<endl;
    }
   for(int i=1;i<=num;i++)
   {
       if(num%i==0)
       {
           cout<<"Factors of"<<num<<"is"<<i<<endl;
       }
   }
}
int main()
{
    int num;
    cout<<"enter the num:";
    cin>>num;
    
    if(num<0)
    {
        cerr<<"Error:the number must be a postive num:";
    }
    else
    {
        factors(num);
    }
    return 0;
}
